  },
  "metadata": {
    "description": "My custom Claude Code plugins for specialized workflows and automation",
    "version": "1.0.0"
  },
  "plugins": [
    {
      "name": "ai-workflow",
      "source": "./plugins/ai-workflow",
      "description": "Linear 기반 AI workflow automation plugin",
      "version": "2.0.0",
      "author": {
        "name": "Seonghyeon Kim",
        "url": "https://github.com/seonghyeonkimm"
      },
      "homepage": "https://github.com/seonghyeonkimm/agents",
      "repository": "https://github.com/seonghyeonkimm/agents",
      "license": "MIT",
      "keywords": [
        "ai",
        "workflow",
        "automation",
        "linear"
      ],
      "category": "development",
      "strict": false,
      "commands": [
        "./commands/setup.md",
        "./commands/add-issue.md"
      ],
      "agents": [],
      "skills": [
        "./skills/create-spec-doc",
        "./skills/create-init-sh"
      ],
      "hooks": {
        "SessionStart": [
          {
            "type": "command",
            "command": "bash .ai-workflow/init.sh 2>/dev/null || echo '{\"hookSpecificOutput\":{\"hookEventName\":\"SessionStart\",\"additionalContext\":\"⚠️ init.sh가 없습니다. /ai-workflow:setup을 먼저 실행해주세요.\"}}'"
          }
        ]
      }
    }
  ]
}
